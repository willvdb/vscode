{
    "atomKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.fontFamily": "'Liberation Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.formatOnPaste": true,
    "editor.insertSpaces": false,
    "editor.snippetSuggestions": "top",
    "editor.wordWrap": "on",
    "zenMode.hideTabs": false,
    "tslint.autoFixOnSave": true,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#f33d3d",
        "activityBarBadge.foreground": "#000000",
        "badge.background": "#7547df",
        "badge.foreground": "#ffffff",
        "activityBar.background": "#482c8a",
        "editor.background": "#1f1f1f",
        "editor.foreground": "#616161",
        "editorBracketMatch.background": "#7547df77",
        "editorBracketMatch.border": "#7547df77",
        "tab.activeBackground": "#7547df",
        "tab.inactiveBackground": "#2e2e2e",
        "tab.inactiveForeground": "#a0a0a0",
        "tab.activeBorder": "#ff4141",
        "editor.selectionBackground": "#482c8a",
        "sideBar.dropBackground": "#482c8a",
        "statusBar.background": "#482c8a",
        "panel.background": "#1d1d1d",
        "sideBar.background": "#242424",
        "selection.background": "#c2a7ff",
    },
    "editor.tokenColorCustomizations": {
        "comments": "#76727c",
        "functions": "#8645ff",
        "keywords": "#745a8c",
        "numbers": "#8a79a9",
        "strings": "#e0c527",
        "types": "#8556c2",
        "variables": "#6a25c4",
        
        "textMateRules": [{
            "scope": "entity.name.tag",
            "settings": {
                "foreground": "#7a15ff",
                "fontStyle": "bold"
            }
        }, {
            "scope": "entity.other.attribute-name",
            "settings": {
                "foreground": "#ff5353",
                "fontStyle": "bold"
            }
        },{
            "scope": "comment",
            "settings": {
                "foreground": "#a5a5a5",
                "fontStyle": "bold"
            }
        },{
            "scope": "entity.other.inherited-class",
            "settings": {
                "foreground": "#d84848",
                "fontStyle": "bold"
            }
        },{
            "scope": "keyword.operator.new",
            "settings": {
                "foreground": "#b9ab2e",
                "fontStyle": "bold"
            }
        },{
            "scope": "keyword.control",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        },{
            "scope": "variable.name",
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        }]

    },
    "window.zoomLevel": 0
}